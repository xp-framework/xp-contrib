package net.xp_forge.aftermath;

import net.xp_forge.aftermath.Deed;

/**
 * Mutate the provided Deed or the Deeds chain
 *
 */
interface Mutation {

  /**
   * Apply this Mutation on the provided Deed
   *
   * @param  net.xp_forge.aftermath.Deed $deed
   * @return void
   */
  public void apply(Deed $deed);
}
